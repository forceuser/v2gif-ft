{
	"name": "video2gif-ftband",
	"version": "1.0.21",
	"main": "index.js",
	"_moduleAliases": {
		"@root": ".",
		"app": "./src/app"
	},
	"devSettings": {
		"port": 8181,
		"serveExternal": false,
		"xopen": "http://localhost:8181",
		"proxy": "http://localhost:8180"
	},
	"scripts": {
		"fix": "eslint --fix ./src/app/",
		"start": "./build-utils/run-esm.js -s ./src/app/server",
		"start-dev": "./build-utils/run-esm.js -s ./src/app/server -p 8180",
		"init": "rm -rf ./node-modules/ > /dev/null && npm i",
		"build": "run-s clean-js bump build:* copyfiles minify:*",
		"dev": "run-p start-dev build-and-minify:less copyfiles dev:less \"dev:js -- {@}\" --",
		"bump": "npm version patch --no-git-tag-version",
		"clean-js": "rm -rf ./dist/js/ > /dev/null",
		"build:js": "NODE_ENV=production BABEL_ENV=production webpack --config ./webpack/production.config.js",
		"build:less": "lessc ./src/less/index.less ./dist/css/index.css && postcss ./dist/css/index.css -r -u autoprefixer",
		"build-dev": "run-p build:templates build:sprites build-and-minify:less copyfiles clean-and-build-dev-js",
		"build-dev-js": "NODE_ENV=development BABEL_ENV=development webpack --config ./webpack/development.config.js",
		"copyfiles": "copyfiles -f ./src/sw.js ./dist/",
		"dev:js": "./build-utils/run-esm.js -s ./build-utils/dev-server.js",
		"dev:less": "watch -p './src/less/*.less' -c 'npm run build-and-minify:less'",
		"minify:less": "foreach -g './dist/css/index.css' -i './dist/css/index.min.css' -x 'uglifycss #{path} --output #{dir}/#{name}.min#{ext}'",
		"build-and-minify:less": "run-p build:less minify:less"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@babel/core": "^7.1.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0",
		"@babel/plugin-proposal-optional-chaining": "^7.0.0",
		"@babel/plugin-proposal-unicode-property-regex": "^7.0.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/plugin-syntax-import-meta": "^7.0.0",
		"@babel/plugin-transform-regenerator": "^7.0.0",
		"@babel/polyfill": "^7.0.0",
		"@babel/preset-env": "^7.1.0",
		"@babel/register": "^7.0.0",
		"bodymovin": "^4.13.0",
		"canvas": "^2.3.1",
		"colors": "^1.1.2",
		"cross-env": "^4.0.0",
		"deepmerge": "^2.2.1",
		"esm": "^3.0.84",
		"express": "^4.16.3",
		"express-http-proxy": "^1.2.0",
		"ffmpeg-binaries": "github:forceuser/node-ffmpeg-binaries",
		"filesize": "^4.1.2",
		"form-data": "^2.3.2",
		"fs-extra": "^7.0.0",
		"giflossy": "^3.0.41",
		"globby": "^8.0.1",
		"gsap": "^1.20.2",
		"html-template-tag": "^1.0.0",
		"ismobilejs": "^0.4.1",
		"isomorphic-fetch": "github:forceuser/isomorphic-fetch",
		"jquery": "^3.3.1",
		"jsdom": "^12.0.0",
		"module-alias": "^2.1.0",
		"moment": "^2.18.1",
		"morgan": "^1.9.0",
		"multer": "^1.3.1",
		"node-fetch": "^2.2.0",
		"regenerator-runtime": "^0.12.1",
		"serve-index": "^1.9.1",
		"serve-static": "^1.13.2",
		"shelljs": "^0.8.3",
		"universal-url": "^2.0.0",
		"uuid": "^3.3.3",
		"yargs": "^12.0.2"
	},
	"devDependencies": {
		"autoprefixer": "^9.1.5",
		"babel-eslint": ">=10.0.1",
		"babel-loader": "^8.0.2",
		"babel-minify": "^0.4.3",
		"babel-plugin-istanbul": "^5.1.0",
		"browser-sync": "^2.24.7",
		"clean-css-cli": "^4.2.1",
		"copyfiles": "^2.1.0",
		"documentation": "^9.0.0",
		"eslint": ">=5.16.0",
		"expose-loader": "^0.7.5",
		"faucet": "0.0.1",
		"find-free-port": "^2.0.0",
		"foreach-cli": "^1.8.0",
		"globby": "^8.0.1",
		"html-escape": "^2.0.0",
		"html-loader": "^0.4.5",
		"http-proxy-middleware": "^0.19.0",
		"imports-loader": "^0.7.1",
		"inject-loader": "^3.0.0",
		"is-glob": "^4.0.0",
		"is-wsl": "^1.1.0",
		"less": "^3.8.1",
		"postcss-cli": "^6.0.0",
		"mustache": "^3.0.0",
		"ngrok": "^3.1.0",
		"npm-run-all": "^4.1.3",
		"nyc": "^13.1.0",
		"opn": "^5.4.0",
		"uglifycss": "0.0.29",
		"watch-cli": "^0.2.3",
		"webpack": "4.20.2",
		"webpack-bundle-analyzer": "^3.0.3",
		"webpack-cli": "3.1.1",
		"webpack-dev-middleware": "3.4.0",
		"webpack-dev-server": "3.1.11",
		"webpack-hot-middleware": "2.24.2",
		"webpack-merge": "4.1.4"
	},
	"description": ""
}
